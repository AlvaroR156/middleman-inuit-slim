p Colour palette grid
  <!-- <%= data.palettes.colours %> -->
<% hash = data.palettes.colours %>
<% hash.each_with_index do |h, i| %>
p Palette Name: <%= h[0] %>
p Colours: <%= h[1] %>
<% end %>

.grid.grid--center.grid--full
  <% data.palettes.colours.lionbox.each_with_index do |f, index| %>
  .grid__item.tv--one-sixth
    .palette.lionbox_<%= index %>

    p.text-banner #<%= f %>
  <% end %>
  .clearfix
  <% data.palettes.colours.lionbox.each_with_index do |f, index| %>
  .grid__item.tv--one-sixth
    .palette.square.lionbox_<%= index %>

    p.text-banner #<%= f %>
  <% end %>
  .clearfix
  <% data.palettes.colours.lionbox.each_with_index do |f, index| %>
  .grid__item.tv--one-sixth
    .palette.circle.lionbox_<%= index %>

    p.text-banner #<%= f %>
  <% end %>
  .clearfix
  <% data.palettes.colours.lionbox.each_with_index do |f, index| %>
  .grid__item.tv--one-sixth
    .palette.rombo.lionbox_<%= index %>

    p.text-banner #<%= f %>
  <% end %>
